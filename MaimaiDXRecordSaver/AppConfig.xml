<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<XmlConfig>
  <General>
    <!-- 目前暂无用途 将来可能会使用(? -->
    <AutoSaveInterval>-1</AutoSaveInterval>
    <!-- 保存乐曲记录的方式 目前可以选择文件或者数据库 -->
    <!-- 目前可用的选项: File, SQLServer -->
    <RecordSaveMethod>File</RecordSaveMethod>
  </General>
  <Database>
    <Server></Server>
    <!-- 你想要使用的数据库名称 -->
    <Name></Name>
    <Username></Username>
    <Password></Password>
    <!-- 是否使用Windows身份验证 (Integrated Security)?-->
    <UseWindowsAuth>False</UseWindowsAuth>
  </Database>
  <WebPageProxy>
    <Enabled>True</Enabled>
    <!-- 运行网页代理的服务器IP地址或域名 -->
    <!-- 网页代理中所有原来的域名(maimai.wahlap.com)将会被替换为此IP或域名+端口号 -->
    <IPBind>127.0.0.1</IPBind>
    <Port>9999</Port>
    <!-- 网页代理HTTP响应的Server标头 -->
    <!-- 设置为空字符串以禁用Server标头 -->
    <ServerHeader>MaimaiDXRecordSaver/{VERSION}</ServerHeader>
    <AllowLogout>False</AllowLogout>
    <AllowChangeName>False</AllowChangeName>
  </WebPageProxy>
  <WechatLoginProxy>
    <Enabled>True</Enabled>
    <Port>9998</Port>
  </WechatLoginProxy>
</XmlConfig>